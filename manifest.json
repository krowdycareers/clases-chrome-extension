{
    "name": "scraping-occ",
    "description": "Obtener avisos",
    "manifest_version": 3,
    "version": "1.0",
    "permissions":["activeTab","scripting", "tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.occ.com.mx/*"
        ],
        "js": ["./contentscript.js"]
      }
    ],
    "action":{
        "default_popup": "./popup/index.html"
    },
    "icons":{
      "32":"./images/icon.png"
  }
}