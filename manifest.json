{
    "manifest_version": 3,
    "name": "OCC Capture",
    "description": "Get Founded Jobs - OCC",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.occ.com.mx/*"
            ],
            "js": [
                "./scripts/contentScript.js"
            ]
        }
    ]
}